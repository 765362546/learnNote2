<% include common/header.ejs%>

<table>
    <tr><th>微博标题</th><th>微博内容</th><th>发表时间</th></tr>
    <% blogs.forEach(function(blog){ %>
        <tr>
            <td><a href="/view/<%=blog._id%>"><%=blog.title%></a></td>
            <td><%=blog.content%></td>
            <td >
                <%= myFormat(blog.time) %>
            </td>
        </tr>

    <%  });  %>
</table>

<% include common/footer.ejs%>